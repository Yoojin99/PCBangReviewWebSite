CREATE TABLE CNinfo
(
ID	CHAR(10) NOT NULL
PCName	CHAR(10)
Time	DATE

PRIMARY KEY (ID)
FOREIGN KEY (ID) REFERENCES USERinfo(ID)

)

CREATE TABLE COUPON
(
CPID	CHAR(10)
CPnum	INT 
info	CHAR(100)

PRIMARY KEY(CPID)
)

CREATE TABLE SWinfo
(
NUM	INT NOT NULL
Gindex	INT NOT NULL

FOREIGN KEY(NUM) REFERENCES PCBinfo(NUM)
)
